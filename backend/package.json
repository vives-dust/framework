{
  "name": "backend",
  "description": "Feathers backend API for the DUST project",
  "version": "0.0.0",
  "homepage": "",
  "private": true,
  "main": "src",
  "keywords": [
    "feathers"
  ],
  "bugs": {},
  "directories": {
    "lib": "src",
    "test": "test/",
    "config": "config/"
  },
  "engines": {
    "node": "^16.0.0",
    "npm": ">= 3.0.0"
  },
  "scripts": {
    "test": "npm run lint && npm run compile && npm run mocha",
    "lint": "eslint src/. test/. --config .eslintrc.json --ext .ts --fix",
    "dev": "ts-node-dev --no-notify src/",
    "start": "node lib/",
    "mocha": "mocha --require ts-node/register --require source-map-support/register \"test/**/*.ts\" --recursive --exit",
    "compile": "shx rm -rf lib/ && tsc"
  },
  "standard": {
    "env": [
      "mocha"
    ],
    "ignore": []
  },
  "types": "lib/",
  "dependencies": {
    "@feathersjs/authentication": "^4.5.14",
    "@feathersjs/authentication-local": "^4.5.14",
    "@feathersjs/authentication-oauth": "^4.5.14",
    "@feathersjs/cli": "^4.8.0",
    "@feathersjs/configuration": "^4.5.12",
    "@feathersjs/errors": "^4.5.12",
    "@feathersjs/express": "^4.5.14",
    "@feathersjs/feathers": "^4.5.12",
    "@feathersjs/socketio": "^4.5.14",
    "@feathersjs/transport-commons": "^4.5.14",
    "@influxdata/influxdb-client": "^1.25.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "feathers-hooks-common": "^6.1.5",
    "feathers-mongoose": "^8.5.1",
    "feathers-permissions": "^2.1.4",
    "feathers-validate-joi": "^4.0.1",
    "helmet": "^4.6.0",
    "mongodb-core": "^3.2.7",
    "mongoose": "^6.3.2",
    "nanoid": "^3.0.0",
    "serve-favicon": "^2.5.0",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/cors": "^2.8.12",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.1",
    "@types/serve-favicon": "^2.5.3",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "axios": "^0.27.2",
    "eslint": "^8.14.0",
    "mocha": "^9.2.2",
    "shx": "^0.3.4",
    "ts-node-dev": "^1.1.8",
    "typescript": "~4.2.3"
  }
}
